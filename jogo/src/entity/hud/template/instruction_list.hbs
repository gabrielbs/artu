<strong>Instruções adicionadas:</strong>
<ul class="dark-scrollable-container">
  {{#each instructions}}
    <li data-name="{{this.name}}">{{this.label}}</li>
  {{/each}}
</ul>
<div class="controls">
  <div class="clickable control-button" id="erase-last-instruction">&#9003;</div>
  <div class="clickable control-button" id="clear-instructions">&#x1f5d1;&#xfe0f;</div>
  <div class="clickable control-button" id="run-instructions">&#x25b6;&#xfe0f;</div>
</div>
